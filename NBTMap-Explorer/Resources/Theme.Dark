<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="Colors.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	<SolidColorBrush x:Key="BackgroundPrimaryBrush" Color="{StaticResource DarkestBackground}"/>
	<SolidColorBrush x:Key="BackgroundSecondaryBrush" Color="{StaticResource Gray950}"/>

	<SolidColorBrush x:Key="SurfacePrimaryBrush" Color="{StaticResource Gray950}"/>
	<SolidColorBrush x:Key="SurfaceSecondaryBrush" Color="{StaticResource Gray900}"/>
	<SolidColorBrush x:Key="SurfaceTertiaryBrush" Color="{StaticResource Gray800}"/>

	<SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource Gray100}"/>
	<SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource Gray400}"/>
	<SolidColorBrush x:Key="TextDisabledBrush" Color="{StaticResource Gray600}"/>
	<SolidColorBrush x:Key="TextOnBrandBrush" Color="{StaticResource White}"/>
	<SolidColorBrush x:Key="TextOnAccentBrush" Color="{StaticResource Gray950}"/>
	<SolidColorBrush x:Key="BorderPrimaryBrush" Color="{StaticResource Gray800}"/>
	<SolidColorBrush x:Key="BorderSecondaryBrush" Color="{StaticResource Gray850}"/>

	<SolidColorBrush x:Key="ActionPrimaryBrush" Color="{StaticResource Principal400}"/>
	<SolidColorBrush x:Key="ActionPrimaryHoverBrush" Color="{StaticResource Principal300}"/>
	<SolidColorBrush x:Key="ActionPrimaryActiveBrush" Color="{StaticResource Principal200}"/>
	<SolidColorBrush x:Key="ActionPrimaryDisabledBrush" Color="{StaticResource Gray800}"/>

	<SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent400}"/>
	<SolidColorBrush x:Key="AccentHoverBrush" Color="{StaticResource Accent300}"/>

	<SolidColorBrush x:Key="StatusErrorBrush" Color="{StaticResource Error400}"/>
	<SolidColorBrush x:Key="StatusErrorBackgroundBrush" Color="{StaticResource Error950}"/>

</ResourceDictionary>